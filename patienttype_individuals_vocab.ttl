@prefix hms: <http://example.org/hms#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

########################################################################
# PatientType Individuals Vocabulary
# Intended vocabulary URI: http://example.org/hms/vocab/PatientTypeIndividuals
#
# Based on PDF ENUM: IDENTIFIED, UNIDENTIFIED, REFERRED
########################################################################

### CLASS REFERENCE
hms:PatientType a owl:Class ;
    rdfs:label "Patient Type" ;
    rdfs:comment "A controlled vocabulary representing types of registered patients." .

### ENUM INDIVIDUALS

hms:IdentifiedPatient a hms:PatientType ;
    rdfs:label "Identified Patient" ;
    rdfs:comment "A patient whose identity is known and verified." .

hms:UnidentifiedPatient a hms:PatientType ;
    rdfs:label "Unidentified Patient" ;
    rdfs:comment "A patient whose identity is unknown at the time of registration." .

hms:ReferredPatient a hms:PatientType ;
    rdfs:label "Referred Patient" ;
    rdfs:comment "A patient referred from another healthcare facility." .
